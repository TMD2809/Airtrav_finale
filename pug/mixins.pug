mixin popularItem(image, priceRange, title, amountReview, time, tourPrice ) 
                .popular-item
                    .popular-image-content 
                        img.popular-image(src="https://d2e5ushqwiltxm.cloudfront.net/wp-content/uploads/sites/62/2016/11/07083553/Destination-Grand_Palace_Bangkok.jpg")
                        .popular-gradient
                        a.popular-button(href="", target="_blank", rel="noopener noreferrer") Must See
                    .popular-content 
                        span.popular-time Sep 28 10:00 AM - 1:00 PM
                        h3.popular-title The Grand Palace
                        .popular-rating     
                            div
                                img.rating-star(src="../Img/Shape.png")
                            div 
                                span.popular-rating-amount 4.8 (12 reviews)
                        